        "if NE>30: DE=2\n",
        "if NE>50: DE=5\n",
        "if NE>100: DE=10\n",
        "# compute RMS average of gradients during epochs\n",
        "for ep in range(1,1+NE):\n",
        "    epo[ep-1]=ep\n",
        "    mgw[ep-1] = np.std(gwE[ep])\n",
        "    mga[ep-1] = np.std(gaE[ep])\n",
        "    mgb[ep-1] = np.std(gbE[ep])\n",
